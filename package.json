{
  "name": "ai-sms-gpt5",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -p tsconfig.json --noEmit",
    "build:web": "vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --max-warnings=0",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "test": "vitest run --reporter=dot --coverage",
    "test:watch": "vitest",
    "test:audio": "vitest run tests/audio/sonic_audio_smoke.test.ts",
    "test:ei": "vitest run tests/smoke/ei_smoke.test.ts",
    "test:smoke": "vitest run tests/audio/sonic_audio_smoke.test.ts tests/smoke/ei_smoke.test.ts",
    "test:timing": "npx tsx tools/verify_z80_timing.ts",
    "test:vdp": "npx tsx tools/verify_vdp_timing.ts",
    "test:all": "npm run test:timing && npm run test:vdp",
    "verify": "npm run test:all",
    "benchmark": "npx tsx test/benchmark.ts",
    "trace": "npx tsx tools/trace_execution.ts",
    "trace:alex": "SMS_ROM=./roms/alex.sms FRAMES=3 OUT=alex_irq_trace.csv npx tsx tools/alex_irq_window_trace.ts",
    "ci": "npm run lint && npm run typecheck && npm run test && npm run verify",
    "web": "npm run build:web && npx http-server dist-web -p 8080 -c-1 --cors",
    "web:dev": "npx http-server . -p 8080 -c-1 --cors",
    "script:analyze-tiles": "tsx scripts/analyze_tiles.ts",
    "script:check-pattern-bases": "tsx scripts/check_pattern_bases.ts",
    "script:debug-patterns": "tsx scripts/debug_patterns.ts",
    "script:debug-tiles": "tsx scripts/debug_tiles_simple.ts",
    "script:debug-tiles-simple": "tsx scripts/debug_tiles_simple.ts",
    "script:debug-vram": "tsx scripts/debug_vram.ts",
    "script:find-tile-data": "tsx scripts/find_tile_data.ts",
    "script:fix-mode": "tsx scripts/fix_mode.ts",
    "dump:tiles": "npx tsx tools/dump_tiles_at_time.ts",
    "dump:sprites": "npx tsx tools/dump_sprites_at_time.ts",
    "trace:install": "brew install mame",
    "trace:sms": "node scripts/mame/run-sms-trace.cjs",
    "trace:last": "ls -1t traces/sms-*.log | head -n 1",
    "mame:run": "node scripts/mame/run-with-timeout.cjs",
    "compare:mame": "npx tsx tools/compare_mame_trace.ts",
    "trace:compare": "npx tsx tools/compare_traces.ts",
    "focus:run": "npx tsx tools/focus_run.ts",
    "rom:im1": "npx tsx tools/roms/make_im1_test.ts",
    "trace:im1": "SMS_ROM=./roms/im1_test.sms TRACE_SECONDS=2 KILL_AFTER_MS=5000 npm run --silent trace:sms",
    "compare:im1": "SMS_ROM=./roms/im1_test.sms ALIGN_PC=0038 MAX_STEPS=50 npm run --silent compare:mame",
    "wb:logo": "WONDERBOY_SMS_ROM=./wonderboy5.sms SMS_BIOS_ROM=./mpr-12808.ic2 vitest run tests/games/wonderboy_boot_logo.test.ts || true; if [ -f out/wonderboy_120f.png ]; then open out/wonderboy_120f.png; else npx tsx tools/probe_wb_120.ts && open out/wb_120_debug.png; fi",
    "test:z80:zexdoc": "npx tsx tools/run_zexdoc.ts",
    "test:z80:compare": "npx tsx tools/compare_zexdoc_results.ts",
    "test:z80:validate": "npm run test:z80:zexdoc && npm run test:z80:compare",
    "trace:generate-mock-mame": "npx tsx tools/generate_mock_mame_traces.ts",
    "trace:capture": "npx tsx tools/trace_cpu.ts",
    "trace:validate:all": "npm run trace:generate-mock-mame && npm run trace:capture -- ./roms/alexkidd.sms --frames 50 --output artifacts/trace_alexkidd.json && npm run trace:compare -- artifacts/trace_alexkidd.json artifacts/mame_traces/alexkidd_mame.json --output artifacts/alexkidd_comparison.md"
  },
  "devDependencies": {
    "@types/node": "^24.4.0",
    "@types/pngjs": "^6.0.5",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^5.0.2",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.5.4",
    "fast-check": "^4.2.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "vite": "^7.1.5",
    "vite-plugin-checker": "^0.11.0",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "canvas": "^3.2.0",
    "pngjs": "^7.0.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  }
}
